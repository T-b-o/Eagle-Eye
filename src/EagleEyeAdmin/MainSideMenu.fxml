            <Button layoutX="10.0" layoutY="213.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="88.0" text="Button" />
            <Button layoutX="10.0" layoutY="264.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="88.0" text="Button" />
            <Button layoutX="10.0" layoutY="316.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="88.0" text="Button" />
            <Button fx:id="btnLogout" layoutX="10.0" layoutY="367.0" mnemonicParsing="false" onAction="#btnLogoutEventHandler" prefHeight="44.0" prefWidth="88.0" text="Log Out" textAlignment="CENTER" />
         </children>
      </AnchorPane>
   </children>
</VBox>
