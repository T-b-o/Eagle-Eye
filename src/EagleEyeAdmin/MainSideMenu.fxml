      fx:controller="EagleEyeAdmin.SideMenuController">
   <children>
      <AnchorPane prefHeight="412.0" prefWidth="94.0">
         <children>
            <Button fx:id="btnSetings" layoutX="10.0" layoutY="7.0" mnemonicParsing="false" onAction="#btnSetingsEventHandler" prefHeight="44.0" prefWidth="88.0" text="Setings" textAlignment="CENTER" />
            <Button fx:id="btnWebcam" layoutX="10.0" layoutY="58.0" mnemonicParsing="false" onAction="#btnWebcamEventHandler" prefHeight="44.0" prefWidth="88.0" text="Webcam" />
